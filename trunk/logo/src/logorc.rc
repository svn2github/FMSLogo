FILEFLAGS VS_FF_PRIVATEBUILD|VS_FF_PRERELEASE
#else
FILEFLAGS 0
#endif

FILEOS VOS__WINDOWS32
FILETYPE VFT_APP
FILESUBTYPE VFT2_UNKNOWN
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904b0"
    BEGIN
      VALUE "CompanyName",      "David Costanzo\0"
      VALUE "FileDescription",  LOCALIZED_FMSLOGO_FILE_DESCRIPTION"\0"
      VALUE "FileVersion",      FMSLOGO_VERSION"\0"
      VALUE "InternalName",     "fmslogo\0"
      VALUE "LegalCopyright",   "Copyright 1995-2009 by Brian Harvey, George Mills, David Costanzo, and Niel Hodgeson\0"
      VALUE "OriginalFilename", "fmslogo.exe\0"
      VALUE "ProductName",      LOCALIZED_GENERAL_PRODUCTNAME"\0"
      VALUE "ProductVersion",   FMSLOGO_VERSION"\0"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", LOCALE, CHARSETID
  END
END


